      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd">

<div>
	<h3>${category.name}</h3>
	<ul>
		<li t:type='loop' t:source='pages' t:value='page'>
			${page.title}
			<a href="?page=${page.id}">show</a>
			<a href="?edit=${page.id}">edit</a>
		</li>
	</ul>
</div>
</t:content>
