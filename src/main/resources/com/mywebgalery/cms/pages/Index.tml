<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">
    <head>
        <meta http-equiv="content-type" content="text/html; charset=utf-8"/>
        <title>${title}</title>
		<link href="${templateCss}" rel="stylesheet" type="text/css" />
		<style type="text/css">
			<t:outputraw value='dynamicCss' />
		</style>
		<script type="text/javascript" src="${templateJs}"></script>
    </head>
    <body>
    <t:messages />
	<t:dynamic t:id='form' template="prop:template"
		t:header='block:header' t:subheader='block:subheader'
		t:left='block:left' t:right='block:right'
		t:content='block:content' t:footer='block:footer' />

	<t:block id='header'>
		header
		<t:loop t:source='tests' t:value='test'>
			<t:delegate to='prop:block' />
		</t:loop>
	</t:block>

	<t:block id='subheader'>
		subheader
	</t:block>

	<t:block id='left'>
		left
	</t:block>

	<t:block id='content'>
		content
	</t:block>

	<t:block id='right'>
		right
	</t:block>

	<t:block id='footer'>
		footer
	</t:block>
</body>
</html>
